{
  "name": "capacitor-delicious-clipboard",
  "version": "0.0.3",
  "description": "More clipboard control for iOS",
  "main": "dist/plugin.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "lint": "npm run prettier -- --check && npm run swiftlint -- lint",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "emilkaiser@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "^5.0.0-beta.3",
    "@capacitor/core": "^5.0.0-beta.3",
    "@capacitor/docgen": "^0.1.1",
    "@capacitor/ios": "^5.0.0-beta.3",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "^2.0.0",
    "@ionic/swiftlint-config": "^1.1.2",
    "eslint": "^7.11.0",
    "prettier": "^2.5.1",
    "prettier-plugin-java": "^1.6.1",
    "rimraf": "^3.0.0",
    "rollup": "^2.29.0",
    "swiftlint": "^1.0.1",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "@capacitor/core": "^5.0.0-beta.3"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorDeliciousClipboard.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/emilkaiser/capacitor-delicious-clipboard"
  },
  "bugs": {
    "url": "https://github.com/emilkaiser/capacitor-delicious-clipboard/issues"
  }
}
